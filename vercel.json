{
  "version": 2,
  "builds": [
    {
      "src": "ml_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "assets/(.*)",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "ml_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}